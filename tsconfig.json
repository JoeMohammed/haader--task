{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./src",
    "paths": {
      "@Components/*": ["/components/*"],
      "@utils/*": ["/utils/*"],
      "@assets/*": ["/assets/*"],
      "@app/*": ["/app/*"],
      "@hooks/*": ["/hooks/*"],
      "@screens/*": ["/screens/*"],
      "@allTypes/*": ["/types/*"],
      "@styles/*": ["/styles/*"],
      "@schema/*": ["/schema/*"],
      "@lib/*": ["/lib/*"],
      "@pages/*": ["/pages/*"],
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
